{
  "name": "skysync-cli",
  "version": "4.12.0",
  "description": "SkySync Command Line Interface",
  "homepage": "http://www.skysync.com",
  "license": "MIT",
  "keywords": [
    "SkySync",
    "Sync",
    "Copy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/portalarchitects/skysync-cli.git"
  },
  "bugs": {
    "url": "https://github.com/portalarchitects/skysync-cli/issues",
    "email": "support@skysync.com"
  },
  "main": "./dist/sdk/index.js",
  "types": "./dist/sdk/index.d.ts",
  "author": {
    "name": "SkySync",
    "email": "developers@skysync.com"
  },
  "contributors": [
    {
      "name": "Shaun Becker",
      "email": "sbecker@skysync.com"
    }
  ],
  "files": [
    "dist/",
    "bin/"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p ./src",
    "prepare": "npm run build",
    "lint": "tslint --config tslint.json 'src/**/*.ts' --exclude '**/node_modules/**'",
    "rebuild": "npm run clean && npm run build",
    "start": "node bin/skysync.js",
    "test": "cross-env TS_NODE_PROJECT=./src/tsconfig.json TIME_ZONE=Pacific/Chatham mocha --timeout 10000 --require ts-node/register src/**/*.test.ts && npm run lint",
    "prepublishOnly": "npm run rebuild"
  },
  "bin": {
    "skysync-cli": "bin/skysync.js"
  },
  "dependencies": {
    "cliff": "^0.1.10",
    "dot-object": "^1.9.0",
    "liftoff": "^3.1.0",
    "lodash": "^4.17.15",
    "open": "6.4.0",
    "request": "^2.88.0",
    "semver": "^6.3.0",
    "tslint": "^5.20.0",
    "v8flags": "^3.1.3",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.8",
    "cross-env": "^6.0.0",
    "debug": "^4.1.1",
    "expect.js": "^0.3.1",
    "extend": "^3.0.2",
    "mocha": "^6.2.0",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^3.6.3"
  }
}
