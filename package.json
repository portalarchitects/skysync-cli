{
  "name": "skysync-cli",
  "version": "4.18.0",
  "description": "SkySync Command Line Interface",
  "homepage": "http://www.skysync.com",
  "license": "MIT",
  "keywords": [
    "SkySync",
    "Sync",
    "Copy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/portalarchitects/skysync-cli.git"
  },
  "bugs": {
    "url": "https://github.com/portalarchitects/skysync-cli/issues",
    "email": "support@skysync.com"
  },
  "main": "./dist/sdk.js",
  "types": "./dist/types/sdk.d.ts",
  "author": {
    "name": "SkySync",
    "email": "developers@skysync.com"
  },
  "contributors": [
    {
      "name": "Shaun Becker",
      "email": "sbecker@skysync.com"
    }
  ],
  "files": [
    "dist/",
    "bin/"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "clean": "rimraf dist publish",
    "build:tsc": "tsc -p ./src",
    "build:webpack": "webpack --config ./build/webpack.js -p",
    "build:publish": "rimraf dist/sdk && ncp ./publish ./dist && rimraf dist/.npmrc dist/package.json",
    "build": "npm run clean && npm run build:tsc && npm run build:webpack && npm run build:publish",
    "prepare": "npm run build",
    "lint": "tslint --config tslint.json 'src/**/*.ts' --exclude '**/node_modules/**'",
    "rebuild": "npm run clean && npm run build",
    "start": "node bin/skysync.js",
    "test": "cross-env TS_NODE_PROJECT=./src/tsconfig.json TIME_ZONE=Pacific/Chatham mocha --timeout 10000 --require ts-node/register src/**/*.test.ts && npm run lint",
    "prepublishOnly": "npm run rebuild"
  },
  "bin": {
    "skysync-cli": "bin/skysync.js"
  },
  "dependencies": {
    "cliff": "^0.1.10",
    "dot-object": "^2.1.4",
    "liftoff": "^3.1.0",
    "lodash": "^4.17.20",
    "open": "7.2.1",
    "request": "^2.88.2",
    "semver": "^7.3.2",
    "v8flags": "^3.2.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.10.2",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "debug": "^4.1.1",
    "expect.js": "^0.3.1",
    "extend": "^3.0.2",
    "generate-package-json-webpack-plugin": "^1.1.2",
    "mocha": "^6.2.1",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.3",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-microsoft-contrib": "^6.2.0",
    "typescript": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2"
  }
}
