{
  "name": "skysync-cli",
  "version": "1.0.0",
  "description": "SkySync Command Line Interface",
  "homepage": "http://www.skysync.com",
  "license": "MIT",
  "keywords": [
    "SkySync",
    "Sync",
    "Copy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/portalarchitects/skysync-cli.git"
  },
  "bugs": {
    "url": "https://github.com/portalarchitects/skysync-cli/issues",
    "email": "support@skysync.com"
  },
  "main": "./dist/sdk/index.js",
  "types": "./dist/sdk/index.d.ts",
  "author": {
    "name": "Portal Architects",
    "email": "developers@skysync.com"
  },
  "contributors": [
    {
      "name": "Shaun Becker",
      "email": "sbecker@skysync.com"
    }
  ],
  "files": [
    "dist/",
    "bin/"
  ],
  "engineStrict": true,
  "engines": {
    "node": ">=8.3.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -p ./src",
    "prepare": "npm run build",
    "lint": "tslint --config tslint.json 'src/**/*.ts' --exclude '**/node_modules/**'",
    "rebuild": "npm run clean && npm run build",
    "start": "node bin/skysync.js",
    "test": "cross-env TS_NODE_PROJECT=./src mocha --compilers ts:ts-node/register src/**/*.test.ts && npm run lint",
    "prepublishOnly": "npm run rebuild"
  },
  "bin": {
    "skysync-cli": "bin/skysync.js"
  },
  "dependencies": {
    "cliff": "^0.1.10",
    "dot-object": "^1.7.0",
    "liftoff": "^2.5.0",
    "open": "0.0.5",
    "prompt": "^1.0.0",
    "request": "^2.87.0",
    "semver": "^5.5.0",
    "tslint": "^5.10.0",
    "v8flags": "^3.1.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/expect.js": "^0.3.29",
    "@types/mocha": "^5.2.0",
    "@types/node": "^8.0.20",
    "cross-env": "^5.1.6",
    "expect.js": "^0.3.1",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.5",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.8.3"
  }
}
