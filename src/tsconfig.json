{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "es6",
		"declaration": true,
		"removeComments": true,
		"sourceMap": false,
		"outDir": "../dist",
		"noImplicitAny": false,
		"allowUnusedLabels" : false,
		"allowUnreachableCode" : false,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"exclude": [
		"node_modules",
		"sdk/http/test-client.ts",
		"**/*.test.ts"
	]
}
